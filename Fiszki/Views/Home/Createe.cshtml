
@{
    ViewData["Title"] = "Createe";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/css/util.css" rel="stylesheet" />
<link href="~/css/main.css" rel="stylesheet" />
<div class="page-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <div class="card-service">
                    <div class="header">

                    </div>
                    <div class="body">
                        <h5 class="text-secondary" style="margin-bottom:5px;">Nowa fiszka</h5>
                        <p style="margin-bottom:5px;">Tutaj możesz stworzyć, opisać i zapisać do wybranej kategorii czy przedmiotu.</p>
                        <a asp-area="" asp-controller="Search" asp-action="Create" class="btn btn-primary">Stwórz</a>

                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="card-service">
                    <div class="header">

                    </div>
                    <div class="body">
                        <h5 class="text-secondary"style="margin-bottom:5px;">Nowy przedmiot</h5>
                        <p style="margin-bottom:5px;">Możesz stworzyć nowy przedmiot w którym zapiszesz fiszki na dany temat.</p>
                        <a asp-area="" asp-controller="" asp-action="" class="btn btn-primary">Stwórz</a>
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="card-service">
                    <div class="header">

                    </div>
                    <div class="body">
                        <h5 class="text-secondary" style="margin-bottom:5px;">Nowa kategoria</h5>
                        <p style="margin-bottom:5px;">Swoje fiszki możesz podzielić na kategorie, więc wszystko będziesz mieć uporządkowane.</p>
                        <a asp-area="" asp-controller="" asp-action="" class="btn btn-primary">Stwórz</a>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- .container -->
</div> <!-- .page-section -->



